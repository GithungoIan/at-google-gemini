{
  "dashboard": {
    "title": "VoiceAI Monitoring",
    "uid": "voiceai-main",
    "version": 1,
    "timezone": "browser",
    "schemaVersion": 16,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Active Calls",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [{
          "expr": "voiceai_active_calls",
          "legendFormat": "Active Calls",
          "refId": "A"
        }]
      },
      {
        "id": 2,
        "title": "Active SMS Sessions",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [{
          "expr": "voiceai_active_sms_sessions",
          "legendFormat": "Active SMS Sessions",
          "refId": "A"
        }]
      },
      {
        "id": 3,
        "title": "Call Rate (per minute)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [{
          "expr": "rate(voiceai_calls_total[1m])",
          "legendFormat": "{{status}}",
          "refId": "A"
        }]
      },
      {
        "id": 4,
        "title": "SMS Rate (per minute)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(voiceai_sms_received_total[1m])",
            "legendFormat": "Received",
            "refId": "A"
          },
          {
            "expr": "rate(voiceai_sms_sent_total{status=\"success\"}[1m])",
            "legendFormat": "Sent (Success)",
            "refId": "B"
          }
        ]
      },
      {
        "id": 5,
        "title": "Call Duration (95th percentile)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [{
          "expr": "histogram_quantile(0.95, rate(voiceai_call_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        }]
      },
      {
        "id": 6,
        "title": "Gemini API Success Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [{
          "expr": "rate(voiceai_gemini_api_calls_total{status=\"success\"}[5m]) / rate(voiceai_gemini_api_calls_total[5m])",
          "legendFormat": "Success Rate",
          "refId": "A"
        }]
      },
      {
        "id": 7,
        "title": "Gemini API Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(voiceai_gemini_api_latency_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(voiceai_gemini_api_latency_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(voiceai_gemini_api_latency_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ]
      },
      {
        "id": 8,
        "title": "JVM Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [{
          "expr": "process_resident_memory_bytes{job=\"voiceai\"}",
          "legendFormat": "Memory (bytes)",
          "refId": "A"
        }]
      }
    ]
  }
}
